{
    "manifest_version": 3,
    "name": "qr code generator",
    "description": "With this extension it is possible to create qr code in a simple and free way.",
    "author": "Adson Nael",
    "version": "0.0.1",
    "icons": {
        "16": "./public/qrcode-16.png",
        "32": "./public/qrcode-32.png",
        "48": "./public/qrcode-48.png",
        "128": "./public/qrcode-128.png"
    },
    "action": {
        "default_popup": "./index.html",
        "default_icon": "./public/qrcode-128.png"
    }
}